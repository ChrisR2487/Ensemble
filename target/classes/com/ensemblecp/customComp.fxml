<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane fx:id="borderPane" prefHeight="1080.0" prefWidth="1920.0" stylesheets="@../../css/compCreatorStyle.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ensemblecp.customCompController">
   <top>
      <HBox prefHeight="39.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <AnchorPane prefHeight="39.0" prefWidth="1931.0">
               <children>
                  <Label text="E N S E M B L E" textFill="WHITE">
                     <font>
                        <Font size="26.0" />
                     </font>
                  </Label>
                   <ImageView fx:id="exitButton" fitHeight="50.0" fitWidth="40.0" layoutX="1866.0" layoutY="-1.0" onMouseClicked="#exitButton_onClick" pickOnBounds="true" preserveRatio="true">
                       <Image url="@../button_imgs/close_button.png" />
                   </ImageView>
               </children>
            </AnchorPane>
         </children>
      </HBox>
   </top>
   <center>
      <VBox prefHeight="443.0" prefWidth="730.0" BorderPane.alignment="CENTER">
         <children>
            <HBox prefHeight="47.0" prefWidth="600.0">
               <children>
                  <Label text="Create your Template:" textFill="WHITE">
                     <font>
                        <Font size="32.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="30.0" top="30.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="31.0" prefWidth="919.0">
               <children>
                  <Button fx:id="addItem" mnemonicParsing="false"  prefHeight="37.0" prefWidth="155.0" text="Add Item">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="30.0" top="30.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <StackPane fx:id="stackPane" prefHeight="196.0" prefWidth="919.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <HBox prefHeight="108.0" prefWidth="919.0">
                           <children>
                              <MenuButton fx:id="menuButton" mnemonicParsing="false" prefHeight="27.0" prefWidth="119.0" text="Entry Type">
                                 <items>
                                    <MenuItem fx:id="stringItem" mnemonicParsing="false" text="String"   /> <!--onAction="#setMenuButton" -->
                                    <MenuItem fx:id="intItem" mnemonicParsing="false" text="Int"  />
                                    <MenuItem fx:id="tableItem" mnemonicParsing="false" text="Table"  />
                                    <MenuItem fx:id="fileItem" mnemonicParsing="false" text="File"/>
                                 </items>
                                 <HBox.margin>
                                    <Insets left="30.0" top="20.0" />
                                 </HBox.margin>
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </MenuButton>
                              <TextField fx:id="inputField" prefHeight="35.0" prefWidth="502.0">
                                 <HBox.margin>
                                    <Insets top="20.0" />
                                 </HBox.margin>
                              </TextField>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </StackPane>
         </children></VBox>
   </center>
   <bottom>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="submit_Button" layoutX="1474.0" layoutY="88.0" mnemonicParsing="false" onAction="#submit_Button_onClick" prefHeight="45.0" prefWidth="372.0" text="Submit">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
