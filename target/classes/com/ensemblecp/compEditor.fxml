<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" stylesheets="@../../css/application.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ensemblecp.CompEditorController">
   <!-- Software title -->
   <Label layoutX="14.0" layoutY="23.0" prefHeight="38.0" prefWidth="198.0" text="E N S E M B L E" textFill="WHITE">
      <font>
         <Font size="26.0" />
      </font>
   </Label>

   <!-- Exit button -->
   <Button fx:id="exitButton" layoutX="1803.0" layoutY="26.0" mnemonicParsing="false" onMouseClicked="#exitButton_onClick" prefHeight="51.0" prefWidth="50.0" style="-fx-background-color: transparent;" styleClass="my-btn-class" stylesheets="@../../css/buttonHoverStyle.css" text="X" textFill="WHITE">
      <font>
         <Font size="36.0" />
      </font>
   </Button>

   <!-- Cancel button -->
   <Button fx:id="cancelButton" alignment="CENTER" contentDisplay="TOP" layoutX="638.0" layoutY="859.0" mnemonicParsing="false" onAction="#cancelButton_onClick" prefHeight="51.0" prefWidth="300.0" styleClass="form-btn-class" stylesheets="@../../css/formButtonStyle.css" text="Cancel">
      <font>
         <Font name="System Bold" size="24.0" />
      </font>
   </Button>

   <!-- Submit button -->
   <Button fx:id="submitButton" alignment="CENTER" contentDisplay="TOP" layoutX="982.0" layoutY="859.0" mnemonicParsing="false" onAction="#submitButton_onClick" prefHeight="51.0" prefWidth="300.0" styleClass="form-btn-class" stylesheets="@../../css/formButtonStyle.css" text="Submit">
      <font>
         <Font name="System Bold" size="24.0" />
      </font>
   </Button>

   <!-- Data entry stuff -->
   <Label layoutX="883.0" layoutY="80.0" text="Enter Data:" textFill="WHITE">
      <font>
         <Font size="32.0" />
      </font>
   </Label>
   <ScrollPane layoutX="713.0" layoutY="140.0" prefHeight="691.0" prefWidth="495.0" style="-fx-background-color: #2a2a2a; -fx-background: #2a2a2a;">
         <VBox fx:id="partDataEntryList" prefWidth="493.0" style="-fx-background-color: #2a2a2a; -fx-padding: 9.0 9.0 9.0 9.0" />
   </ScrollPane>
</AnchorPane>
