<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" stylesheets="@../../css/application.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ensemblecp.CompSelectorController">
   <children>
      <!-- Software title -->
      <Label layoutX="14.0" layoutY="23.0" prefHeight="38.0" prefWidth="198.0" text="E N S E M B L E" textFill="WHITE">
         <font>
            <Font size="26.0" />
         </font>
      </Label>

      <!-- Exit button -->
      <Button fx:id="exitButton" layoutX="1866.0" layoutY="-1.0" mnemonicParsing="false" onMouseClicked="#exitButton_onClick" prefHeight="51.0" prefWidth="50.0" style="-fx-background-color: transparent;" styleClass="my-btn-class" stylesheets="@../../css/buttonHoverStyle.css" text="X" textFill="WHITE">
         <font>
            <Font size="36.0" />
         </font>
      </Button>

      <!-- Submit & Cancel Buttons -->
      <Button fx:id="submit_Button" layoutX="1474.0" layoutY="88.0" mnemonicParsing="false" onAction="#submitButton_onClick" prefHeight="45.0" prefWidth="300.0" text="Submit">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="cancelButton" layoutX="1159.0" layoutY="88.0" mnemonicParsing="false" onAction="#cancelButton_onClick" prefHeight="45.0" prefWidth="300.0" text="Cancel">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
