<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" stylesheets="@../../css/compCreatorStyle.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ensemblecp.CompController">
   <Label layoutX="54.0" layoutY="75.0" text="Select an Option:" textFill="WHITE">
      <font>
         <Font size="32.0" />
      </font>
   </Label>
   <RadioButton layoutX="960.0" layoutY="152.0" mnemonicParsing="false" onAction="#showComps" text="Use Existing Template" textFill="WHITE">
      <font>
         <Font size="24.0" />
      </font>
   </RadioButton>
   <RadioButton layoutX="153.0" layoutY="152.0" mnemonicParsing="false" onAction="#showCustomComp" text="Edit Custom Templates" textFill="WHITE">
      <font>
         <Font size="24.0" />
      </font>
   </RadioButton>
   <Label layoutX="8.0" layoutY="6.0" text="E N S E M B L E" textFill="WHITE">
      <font>
         <Font name="System Bold" size="26.0" />
      </font>
   </Label>
   <ImageView fx:id="exitButton" fitHeight="50.0" fitWidth="40.0" layoutX="1852.0" layoutY="5.0" onMouseClicked="#exitButton_onClick" pickOnBounds="true" preserveRatio="true">
      <Image url="@../button_imgs/close_button.png" />
   </ImageView>
   <VBox layoutX="55.0" layoutY="322.0" prefHeight="399.0" prefWidth="1329.0">
      <children>
         <TableView fx:id="existingTable" prefHeight="370.0" prefWidth="527.0" visible="false"> <!-- Width=606 -->
            <columns>
               <TableColumn fx:id="titleColumn" prefWidth="175.0" text="Title" />
               <TableColumn fx:id="templateColumn" prefWidth="431.0" text="Template" />
            </columns>
         </TableView>
      </children>
   </VBox>
</AnchorPane>
