<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="root" prefHeight="1080" prefWidth="1920" stylesheets="@../../css/application.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ensemblecp.DashboardController">
   <children>
      <VBox layoutX="27.0" layoutY="126.0" prefHeight="575.0" prefWidth="191.0" AnchorPane.leftAnchor="27.0">
         <children>
            <HBox onMouseClicked="#dashButton_onClick" prefHeight="25.0" prefWidth="160.0">
               <children>
                  <ImageView fitHeight="36.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true" style="-fx-translate-y: 9;">
                     <Image url="@../button_imgs/dashboard-icon.png" />
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </ImageView>
                  <Button mnemonicParsing="false" onAction="#dashButton_onClick" prefHeight="43.0" prefWidth="143.0" style="-fx-background-color: transparent;" text="Dashboard" textFill="WHITE">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" />
                     </HBox.margin>
                     <font>
                        <Font size="22.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <HBox onMouseClicked="#projListButton_onClick" prefHeight="28.0" prefWidth="160.0">
               <children>
                  <ImageView fitHeight="27.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true" style="-fx-translate-y: 9;">
                     <Image url="@../button_imgs/projlist-icon.png" />
                  </ImageView>
                  <Button mnemonicParsing="false" onAction="#projListButton_onClick" prefHeight="31.0" prefWidth="117.0" style="-fx-background-color: transparent;" text="Projects" textFill="WHITE">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" />
                     </HBox.margin>
                     <font>
                        <Font size="22.0" />
                     </font></Button>
               </children>
            </HBox>
            <HBox onMouseClicked="#archiveButton_onClick" prefHeight="35.0" prefWidth="160.0">
               <children>
                  <ImageView fitHeight="31.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true" style="-fx-translate-y: 9;">
                     <Image url="@../button_imgs/archive-icon.png" />
                  </ImageView>
                  <Button mnemonicParsing="false" onAction="#archiveButton_onClick" prefHeight="27.0" prefWidth="130.0" style="-fx-background-color: transparent;" text="Archives" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                     <font>
                        <Font size="22.0" />
                     </font></Button>
               </children>
            </HBox>
         </children>
      </VBox>
      <Label layoutX="14.0" layoutY="23.0" prefHeight="38.0" prefWidth="198.0" text="E N S E M B L E" textFill="WHITE">
         <font>
            <Font size="26.0" />
         </font></Label>
      <Label layoutX="212.0" layoutY="61.0" prefHeight="38.0" prefWidth="221.0" text="Dashboard" textFill="WHITE">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <VBox layoutX="1439.0" layoutY="119.0" prefHeight="607.0" prefWidth="427.0">
         <children>
            <Label text="Notifications" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <TableView fx:id="projectTable" layoutX="212.0" layoutY="798.0" prefHeight="200.0" prefWidth="1181.0">
        <columns>
          <TableColumn fx:id="statusColumn" prefWidth="99.0" text="Status" />
          <TableColumn fx:id="titleColumn" prefWidth="86.0" text="Title" />
            <TableColumn fx:id="remainColumn" prefWidth="138.0" text="Remaining Budget" />
            <TableColumn fx:id="kickoffColumn" prefWidth="103.0" text="Start Date" />
            <TableColumn fx:id="deadlineColumn" prefWidth="94.0" text="End Date" />
        </columns>
      </TableView>
      <Label layoutX="212.0" layoutY="752.0" prefHeight="38.0" prefWidth="141.0" text="Projects" textFill="WHITE">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Button layoutX="310.0" layoutY="760.0" mnemonicParsing="false" onAction="#add_onClick" text="+" />
      <VBox layoutX="1439.0" layoutY="798.0" prefHeight="200.0" prefWidth="443.0">
         <children>
            <HBox prefHeight="26.0" prefWidth="443.0">
               <children>
                  <Label text="All Members" textFill="WHITE">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children>
      </VBox>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" />
      <!-- Close button -->
      <ImageView fx:id="exitButton" layoutX="1814.0" layoutY="30.0" onMouseClicked="#exitButton_onClick">
         <Image url="@../button_imgs/close_button.png" />
      </ImageView>
   </children>
</AnchorPane>
